{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "lib": ["dom", "es2017"],
    "baseUrl": ".",
    "moduleResolution": "node",
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "isolatedModules": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "strictNullChecks": false
  },
  "include": [
    "components/selectors/Button/ButtonSettings.jsx",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/selectors/Button/index.jsx",
    "components/selectors/Video/index.jsx",
    "components/selectors/Text/TextSettings.js",
    "components/selectors/Video/VideoSettings.js",
    "components/selectors/Text/index.jsx",
    "components/editor/Toolbar/ToolbarItem.js",
    "components/editor/Toolbar/Date.jsx",
    "components/editor/Toolbar/index.jsx",
    "components/editor/Toolbar/ListStyle.jsx",
    "components/editor/Toolbar/InputBox.jsx",
    "components/editor/Toolbar/SelectTag.jsx",
    "components/editor/Toolbar/SetLink.jsx",
    "components/editor/Toolbar/ToolbarRadio.jsx",
    "components/editor/Toolbar/ToolbarDropdown.jsx",
    "components/editor/Toolbar/ToolbarSection.jsx",
    "components/editor/Toolbar/ToolbarTextInput.jsx",
    "components/selectors/Container/index.jsx",
    "components/selectors/Container/ContainerSettings.js",
    "pages/index.jsx"
  ],
  "exclude": ["node_modules"]
}
